{
  "name": "wp-3d-model-viewer",
  "version": "1.0.0",
  "description": "A powerful WordPress plugin for displaying interactive 3D models in the browser with WebGL technology, supporting GLTF, GLB, OBJ, and other popular 3D formats.",
  "main": "wp-3d-model-viewer.php",
  "scripts": {
    "dev": "npm run watch",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss admin/css/wp-3d-model-viewer-admin.css public/css/wp-3d-model-viewer-public.css --use autoprefixer --dir dist/css/",
    "build:js": "webpack --mode=production",
    "watch": "webpack --mode=development --watch",
    "lint": "eslint admin/js/ public/js/ --ext .js",
    "lint:fix": "eslint admin/js/ public/js/ --ext .js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"**/*.{js,css,json,md}\"",
    "zip": "npm run build && wp dist-archive",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karalumpas/wp-3d-model-viewer.git"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "3d",
    "model",
    "viewer",
    "webgl",
    "three.js",
    "gltf",
    "ar",
    "augmented-reality",
    "model-viewer"
  ],
  "author": "Karalumpas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Karalumpas/wp-3d-model-viewer/issues"
  },
  "homepage": "https://github.com/Karalumpas/wp-3d-model-viewer#readme",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@wordpress/babel-preset-default": "^7.31.0",
    "@wordpress/eslint-plugin": "^17.0.0",
    "@wordpress/prettier-config": "^3.0.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@google/model-viewer": "^3.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/js/setup.js"
    ],
    "testMatch": [
      "<rootDir>/tests/js/**/*.test.js"
    ]
  },
  "prettier": "@wordpress/prettier-config",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
