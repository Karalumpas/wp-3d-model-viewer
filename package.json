{
  "name": "wp-3d-model-viewer",
  "version": "1.0.0",
  "description": "A powerful WordPress plugin for displaying interactive 3D models in the browser with WebGL technology, supporting GLTF, GLB, OBJ, and other popular 3D formats.",
  "main": "wp-3d-model-viewer.php",
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "npm run build:css:dev && npm run build:css:prod",
    "build:css:dev": "postcss admin/css/src/admin.css --output admin/css/wp-3d-model-viewer-admin.css --map",
    "build:css:prod": "cross-env NODE_ENV=production postcss admin/css/src/admin.css --output admin/css/wp-3d-model-viewer-admin.min.css --no-map",
    "build:js": "npm run build:js:dev && npm run build:js:prod",
    "build:js:dev": "esbuild admin/js/src/admin.js --bundle --outfile=admin/js/wp-3d-model-viewer-admin.js --sourcemap",
    "build:js:prod": "esbuild admin/js/src/admin.js --bundle --outfile=admin/js/wp-3d-model-viewer-admin.min.js --minify",
    "watch": "npm run watch:css & npm run watch:js",
    "watch:css": "postcss admin/css/src/admin.css --output admin/css/wp-3d-model-viewer-admin.css --map --watch",
    "watch:js": "esbuild admin/js/src/admin.js --bundle --outfile=admin/js/wp-3d-model-viewer-admin.js --sourcemap --watch",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"admin/css/src/**/*.css\"",
    "lint:js": "eslint admin/js/ public/js/ --ext .js",
    "lint:fix": "eslint admin/js/ public/js/ --ext .js --fix",
    "format": "npm run format:css && npm run format:js",
    "format:css": "prettier --write \"admin/css/src/**/*.css\"",
    "format:js": "prettier --write \"admin/js/src/**/*.js\" \"public/js/src/**/*.js\"",
    "clean": "rimraf admin/css/wp-3d-model-viewer-admin.{css,min.css,css.map} admin/js/wp-3d-model-viewer-admin.{js,min.js,js.map} public/js/wp-3d-model-viewer-public.{js,min.js,js.map}",
    "dev": "npm run clean && npm run watch",
    "prod": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "zip": "npm run build && wp dist-archive",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karalumpas/wp-3d-model-viewer.git"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "3d",
    "model",
    "viewer",
    "webgl",
    "three.js",
    "gltf",
    "ar",
    "augmented-reality",
    "model-viewer"
  ],
  "author": "Karalumpas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Karalumpas/wp-3d-model-viewer/issues"
  },
  "homepage": "https://github.com/Karalumpas/wp-3d-model-viewer#readme",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@wordpress/babel-preset-default": "^7.31.0",
    "@wordpress/eslint-plugin": "^17.0.0",
    "@wordpress/prettier-config": "^3.0.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cssnano": "^6.1.2",
    "esbuild": "^0.19.8",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-calc": "^9.0.1",
    "postcss-cli": "^11.0.1",
    "postcss-custom-properties": "^13.3.12",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.2.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@google/model-viewer": "^3.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/js/setup.js"
    ],
    "testMatch": [
      "<rootDir>/tests/js/**/*.test.js"
    ]
  },
  "prettier": "@wordpress/prettier-config",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
